{
  "manifest_version": 2,
  "name": "Walmart Dev Toolkit",
  "description": "Chrome extension to help walmart developers",
  "version": "1.0.0",
  "icons": {
    "128": "icon.png"
  },
  "browser_action": {
    "default_icon": {
      "32": "icon.png"
    },
    "default_popup": "index.html/#popup",
    "default_title": "Open Popup"
  },
  "background": {
    "persistent": false,
    "scripts": ["background.js"]
  },
  "permissions": ["storage", "<all_urls>", "cookies", "tabs"]
}
